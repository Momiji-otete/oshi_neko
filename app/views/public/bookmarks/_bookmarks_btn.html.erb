<% unless current_end_user.own_cats?(cat) %>
  <% if current_end_user.bookmarked?(cat) %>
    <%= link_to cat_bookmarks_path(cat), method: :delete, remote: true do %>
      <i class="fa-solid fa-star"></i>
    <% end %>
  <% else %>
    <%= link_to cat_bookmarks_path(cat), method: :post, remote: true do %>
      <i class="fa-regular fa-star"></i>
    <% end %>
  <% end %>
<% end %>